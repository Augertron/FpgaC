library ieee;
use ieee.std_logic_1164.all;
use ieee.std_logic_arith.all;
use ieee.std_logic_unsigned.all;

-- fpgac 1.0.beta-3A  SVN: $Revision: 53 $ Tue Aug  1 21:10:16 2006

entity <stdin is port(
	CLK : in std_logic;
	RESET : in std_logic;
	o : out std_logic
);
end;

architecture arch_<stdin of <stdin is

	signal initializers_2_Running : std_logic;
	signal initializers_2_T1__Start : std_logic;
	signal initializers_2__state_C1 : std_logic;
	signal o : std_logic;
	signal FFin_initializers_2_Running : std_logic;
	signal FFin_initializers_2_T1__Start : std_logic;
	signal FFin_o : std_logic;


begin

	o <= o;

	FFin_initializers_2_Running <= '1';
	FFin_initializers_2_T1__Start <= not initializers_2_Running;
	initializers_2__state_C1 <= initializers_2_T1__Start;
	FFin_o <= '1';


process(RESET, CLK) begin


	if (RESET = '1') then

		initializers_2_Running <= '0';
		initializers_2_T1__Start <= '0';
		o <= '0';

	elsif (CLK'event and CLK = '1') then

		initializers_2_Running <= FFin_initializers_2_Running;
		initializers_2_T1__Start <= FFin_initializers_2_T1__Start;
		if (initializers_2__state_C1 = '1') then
			o <= FFin_o;
		end if;
	end if;

end process;

end arch_<stdin;
